/* pages/createTesk/createTesk.wxss */
.teskInfo{
  width: 100%;
}
picker{
  width: 70%;
  text-align: right;
  position: relative;
}
picker text{
  display: inline-block;
  margin-right: 20rpx;
}
.teskInfo .infoRow picker image{
  position: absolute;
  right: -5rpx;
  top: 50%;
  margin-top: -8rpx;
  margin-left: 30rpx;
  height: 16rpx;
  width: 28rpx;
  color: #333;
  transform: rotate(270deg);
}
.teskBookImg{
  width: 100%;
  height: 272rpx;
  text-align: center;
}
.teskBookImg image{
  width: 216rpx;
  height: 272rpx;
  border-radius: 12rpx;
  margin-top: 82rpx;
}
.btn{
  margin-top: 262rpx;
}
